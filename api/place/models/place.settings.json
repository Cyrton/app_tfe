{
  "kind": "collectionType",
  "collectionName": "places",
  "info": {
    "name": "place",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "city": {
      "via": "places",
      "model": "city"
    },
    "shortDescription": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "numberOfApproval": {
      "type": "integer",
      "private": false,
      "default": 0,
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "cult",
        "history",
        "tourism"
      ],
      "default": "history",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "comments": {
      "via": "place",
      "collection": "comment"
    },
  "audioDescription": {
    "model": "file",
    "via": "related",
    "allowedTypes": [
      "images",
      "files",
      "videos"
    ],
    "plugin": "upload",
    "required": false,
    "pluginOptions": {}
  }
  }
}
