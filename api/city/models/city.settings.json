{
  "kind": "collectionType",
  "collectionName": "cities",
  "info": {
    "name": "city",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "places": {
      "via": "city",
      "collection": "place"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "description": {
      "type": "richtext"
    },
    "population": {
      "type": "biginteger"
    },
    "countryCode": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "shortDescription": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "small",
        "medium",
        "large"
      ],
      "default": "small"
    },
    "imageGallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "events": {
      "via": "city",
      "collection": "event"
    }
  }
}
